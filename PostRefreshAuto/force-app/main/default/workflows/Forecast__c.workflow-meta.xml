    <rules>
        <fullName>Forecast Publish</fullName>
        <actions>
            <name>Forecast_Outbound</name>
            <type>OutboundMessage</type>
        </actions>
        <active>false</active>
        <description>Outbound Message Triggered  When Forecast Opportunity Stage or Opportunity Service Line Item Stage  equals to 3. Solutioning   and Above &amp; Last Modified User Is Not  an Integration User.</description>
        <formula>AND ( 	OR 	( 		AND 		(		 			OR 			( 				ISPICKVAL(Opportunity1__r.StageName, &quot;3. Solutioning&quot;), 				ISPICKVAL(Opportunity1__r.StageName, &quot;4. End-Game&quot;), 				ISPICKVAL(Opportunity1__r.StageName, &quot;5. Negotiation&quot;), 				ISPICKVAL(Opportunity1__r.StageName, &quot;Lost&quot;), 				ISPICKVAL(Opportunity1__r.StageName, &quot;Won&quot;), 				ISPICKVAL(Opportunity1__r.StageName, &quot;Client Withdraw&quot;), 				ISPICKVAL(Opportunity1__r.StageName, &quot;Cognizant Withdraw&quot;), 				AND 				( 					ISPICKVAL(Opportunity1__r.StageName, &quot;Duplicate&quot;), 					Opportunity1__r.Is_Qualified__c = True 				) 			), 			Opportunity1__r.Vertical_TCV__c &gt; 0 		), 		AND 		( 			OR 			(		 				ISPICKVAL(Opportunity_Service_Line_Item__r.Stage__c , &quot;3. Solutioning&quot;), 				ISPICKVAL(Opportunity_Service_Line_Item__r.Stage__c, &quot;4. End-Game&quot;), 				ISPICKVAL(Opportunity_Service_Line_Item__r.Stage__c, &quot;5. Negotiation&quot;), 				ISPICKVAL(Opportunity_Service_Line_Item__r.Stage__c, &quot;Lost&quot;), 				ISPICKVAL(Opportunity_Service_Line_Item__r.Stage__c, &quot;Won&quot;), 				ISPICKVAL(Opportunity_Service_Line_Item__r.Stage__c, &quot;Client Withdraw&quot;), 				ISPICKVAL(Opportunity_Service_Line_Item__r.Stage__c, &quot;Cognizant Withdraw&quot;), 				AND 				( 					ISPICKVAL(Opportunity_Service_Line_Item__r.Stage__c, &quot;Duplicate&quot;), 					Opportunity_Service_Line_Item__r.Opportunity1__r.Is_Qualified__c = True 				) 			), 			Opportunity_Service_Line_Item__r.TCV_Share__c &gt; 0 		) 	), 	$Profile.Name  &lt;&gt; &apos;Integration User&apos;)</formula>
        <triggerType>onAllChanges</triggerType>
